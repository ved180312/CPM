<h2>edit floor</h2>
<% if @floor.errors.any? %>
  <h2>The following errors prevented the floor from being saved</h2>
  <ul>
    <% @floor.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>
<%= form_with(model: @floor, local: true) do |f| %>
      <p> 
        <%= f.label :number_of_floor %><br/> 
        <%= f.number_field :number_of_floor,Placeholder: "enter name" %>
      </p>

      <p>
        <%= f.submit %>
      </p>

<% end %>